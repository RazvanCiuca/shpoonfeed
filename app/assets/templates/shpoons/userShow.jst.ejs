<div class='row'>
  <div class="col-md-9 col-sm-9 col-xs-8">    
    <div class='row'>
      <% i = 5 %>
      <div class="col-md-<%= i %> col-sm-<%= i %> col-xs-<%= i %>">
        <button id="get-location" class="btn btn-lg btn-success btn-block">Tell me where to eat!</button> 
        <button id="toggle-map" class="btn btn-warning btn-block">Map</button>
        <br><div id="map-canvas" class="well hidden"></div><br>
        <div id="reject-pile" class="well"></div>
      </div>
      <div id="results" class="col-md-<%= 12 - i %> col-sm-<%= 12 - i %> col-xs-<%= 12 - i %>">
        A Feast awaits you at:<br>
        
      </div>
    </div>
  </div>
  
  <div class="col-md-3 col-sm-3 col-xs-4 pull-right">
    <div class="list-group">
      <a href="#friends" class="list-group-item active">
        <h3 class="list-group-item-heading">Friends</h3>
      </a>
      <div id="ticker">
        <% if (friends.length === 0) { %>
          <div class="list-group-item well">
            <h4 class="list-group-item-heading">
              <a href="#friends">Add some Friends</a>           
            </h4>
          </div> 
        <% } %>
        <% friends.each(function(user){ %>  
          
          <div class="list-group-item draggable-friend" data-id="<%=user.escape('id')%>">
            <h4 class="list-group-item-heading">
              <%= user.escape('username') %>
            </h4>
          </div> 
        <% }) %>       
      </div>
      <a class="list-group-item active">
        <h3 class="list-group-item-heading"></h3>
      </a>     
    </div>
    <div class="list-group" id="add-to-party">
      <a class="list-group-item active">
        <h3 class="list-group-item-heading">Party</h3>
      </a>
      <div id="ticker">
        <% if (party.length === 0) { %>
          <div class="list-group-item well">
            <h4 class="list-group-item-heading">
              Drag Friends Here!            
            </h4>
          </div> 
        <% } %>
        <% party.each(function(user){ %>  
          <div class="list-group-item draggable-friend">
            <h4 class="list-group-item-heading">
              <%= user.escape('username') %>
            </h4>
          </div> 
        <% }) %>       
      </div>
      <a class="list-group-item active">       
          <button id="make-happy" class="btn btn-success btn-block">Make Everyone Happy</button>        
      </a>     
    </div>
    
  </div>
</div>










